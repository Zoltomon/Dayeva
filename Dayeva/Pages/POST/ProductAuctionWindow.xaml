<Window x:Class="Dayeva.Pages.POST.ProductAuctionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dayeva.Pages.POST"
        mc:Ignorable="d"
        Title="ProductAuctionWindow" Height="450" Width="400">
    <Grid>
        <StackPanel>
            <TextBlock Text="Введите название продукции:"/>
            <TextBox Name="TxbName"/>
            <TextBlock Text="Стартовая цена:"/>
            <TextBox Name="TxbStartPrice"/>
            <Button
                Click="BtnSave_Click"
                Content="Сохранить"
                Name="BtnSave"/>
        </StackPanel>
    </Grid>
</Window>
